{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Block Height",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "tendermint_consensus_latest_block_height",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "fixed",
            "fixedColor": "#2596be"
          },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10
          },
          "unit": "none"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Validator Voting Power",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "tendermint_validator_power",
          "legendFormat": "{{validator}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "fixed",
            "fixedColor": "#ffffff"
          },
          "custom": {
            "lineWidth": 2
          },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Average Restake Ratio",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "avg(blocrestake_restake_ratio)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "fixed",
            "fixedColor": "#2596be"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "#f2495c",
                "value": null
              },
              {
                "color": "#2596be",
                "value": 0.5
              }
            ]
          },
          "unit": "percentunit"
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal"
      },
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Gas Price & Base Fee",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "feemarket_base_fee",
          "legendFormat": "Base Fee",
          "refId": "A"
        },
        {
          "expr": "feemarket_min_gas_price",
          "legendFormat": "Min Gas",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 8,
        "y": 8
      }
    },
    {
      "id": 5,
      "type": "bargauge",
      "title": "Consensus Health",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "sum(rate(tendermint_consensus_precommits_total[1m]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "fixed",
            "fixedColor": "#2596be"
          },
          "max": 1,
          "min": 0
        },
        "overrides": []
      },
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 16,
        "y": 8
      }
    }
  ],
  "schemaVersion": 36,
  "style": "dark",
  "tags": [
    "lyfebloc",
    "validators"
  ],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "title": "Lyfebloc Network \u2014 Powering Universal Restaking",
